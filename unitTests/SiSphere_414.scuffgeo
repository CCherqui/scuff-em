# .scuffgeo file used for SCUFF-EM unit tests on a single Si sphere

MATERIAL SILICON
    epsf = 1.035;      # \epsilon_infinity
    eps0 = 11.87;      # \epsilon_0 
    wp = 6.6e15;       # \plasmon frequency
    Eps(w) = epsf + (eps0-epsf)/(1-(w/wp)^2);
ENDMATERIAL

OBJECT TheSphere
	MESHFILE UnitTestSphere_414.msh
	MATERIAL Silicon
ENDOBJECT
